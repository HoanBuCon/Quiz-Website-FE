DATABASE_URL="your-development-database-url"
JWT_SECRET="your-development-jwt-secret" # OR DELETE THIS LINE
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000
BASE_PATH=/api

# SMTP
SMTP_HOST=your_smtp_host
SMTP_PORT=your_smtp_port
SMTP_SECURE=false/ # true if your SMTP server requires TLS
SMTP_USER=your-smtp@domain.com
SMTP_PASS=your-smtp-password
SMTP_FROM="Quiz App <your-smtp@domain.com>"
SMTP_TLS_REJECT_UNAUTHORIZED=true
OTP_TTL_SECONDS=300
OTP_THROTTLE_SECONDS=60
OTP_MAX_ATTEMPTS=5